<extension xmlns="http://ns.adobe.com/air/extension/20.0">
  <id>com.marpies.ane.gameServices</id>
  <versionNumber>1.0.2</versionNumber>
  <platforms>
    <platform name="iPhone-ARM">
        <applicationDeployment>
            <nativeLibrary>libGameServices.a</nativeLibrary>
            <initializer>GameServicesInitializer</initializer>
            <finalizer>GameServicesFinalizer</finalizer>
        </applicationDeployment>
    </platform>

    <platform name="iPhone-x86">
        <applicationDeployment>
            <nativeLibrary>libGameServices.a</nativeLibrary>
            <initializer>GameServicesInitializer</initializer>
            <finalizer>GameServicesFinalizer</finalizer>
        </applicationDeployment>
    </platform>

    <platform name="Android-ARM">
        <applicationDeployment>
            <nativeLibrary>libGameServices.jar</nativeLibrary>
            <initializer>com.marpies.ane.gameservices.GameServicesExtension</initializer>
            <finalizer>com.marpies.ane.gameservices.GameServicesExtension</finalizer>
        </applicationDeployment>
    </platform>

    <platform name="Android-x86">
        <applicationDeployment>
            <nativeLibrary>libGameServices.jar</nativeLibrary>
            <initializer>com.marpies.ane.gameservices.GameServicesExtension</initializer>
            <finalizer>com.marpies.ane.gameservices.GameServicesExtension</finalizer>
        </applicationDeployment>
    </platform>

    <platform name="default">
        <applicationDeployment/>
    </platform>
  </platforms>
</extension>
